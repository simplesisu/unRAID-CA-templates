<Container>
  <Name>autogpt-gotty</Name>
  <Repository>cdukes/autogpt-gotty</Repository>
  <Registry>https://hub.docker.com/r/cdukes/autogpt-gotty</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/claytondukes/autogpt-docker</Support>
  <Project>https://github.com/claytondukes/autogpt-docker.git</Project>
  <Overview>AutoGPT-Gotty - A web-based terminal for AutoGPT</Overview>
  <Category>Productivity:</Category>
  <TemplateURL>https://raw.githubusercontent.com/simplesisu/autogpt-docker/master/autogpt-gotty.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/simplesisu/autogpt-docker/master/icon.png</Icon>
  <Version>1.0.0</Version>
  <Date>2023-04-17</Date>
  <!-- Add necessary configuration options here -->
  <Config>
    <Name>OPENAI_API_KEY</Name>
    <Target>/app/.env</Target>
    <Default>YOUR_DEFAULT_VALUE</Default>
    <Type>Variable</Type>
    <Description>Enter your OpenAI API key</Description>
    <Display>OpenAI API Key</Display>
  </Config>
 <WebUI>http://[IP]:[PORT:17442]/</WebUI>
</Container>
